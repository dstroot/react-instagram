{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.2",
    "bootstrap": "^4.3.1",
    "encoding": "^0.1.12",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "jest-canvas-mock": "^2.2.0",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "react": "^0.0.0-experimental-b53ea6ca0",
    "react-dom": "^0.0.0-experimental-b53ea6ca0",
    "react-query": "^0.3.6",
    "react-scripts": "^3.2.0",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lambda": "netlify-lambda serve src/lambda"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.stories.js",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/setupTests.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "devDependencies": {
    "http-proxy-middleware": "^0.20.0",
    "jest-fetch-mock": "^2.1.2",
    "netlify-lambda": "^1.6.3",
    "prettier": "^1.19.1",
    "pretty": "^2.0.0"
  }
}
